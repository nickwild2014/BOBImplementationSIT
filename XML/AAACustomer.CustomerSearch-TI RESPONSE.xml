<?xml version="1.0" standalone="yes"?><ServiceResponse xmlns:m='urn:messages.service.ti.apps.tiplus2.misys.com' xmlns='urn:control.services.tiplus2.misys.com' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'>
	<ResponseHeader>
		<Service>Customer</Service>
		<Operation>CustomerSearch</Operation>
		<Status>${Status}</Status>
		<Details>
			<Error>${Error}</Error>
			<Warning>${Warning}</Warning>
			<Info>${Info}</Info>
		</Details>
		<CorrelationId>CorrelationId</CorrelationId>
	</ResponseHeader>
	<m:CustomerSearchResponse>
		<m:CustomerSearchResults>
			<m:CustomerSearchResult>
				<m:CustomerMnemonic>${CustomerMnemonic}</m:CustomerMnemonic>
				<m:FullName>${FullName}</m:FullName>
				<m:CustomerNumber>${CustomerNumber}</m:CustomerNumber>
				<m:CountryOfResidence>${CountryOfResidence}</m:CountryOfResidence>
				<m:AccountOfficer>${AccountOfficer}</m:AccountOfficer>
				<m:Blocked>${Blocked}</m:Blocked>
				<m:Group>${Group}</m:Group>
			</m:CustomerSearchResult>
		</m:CustomerSearchResults>
		<m:AdditionalResults>${AdditionalResults}</m:AdditionalResults>
	</m:CustomerSearchResponse>
</ServiceResponse>
