				</ns4:Details>
			</ns4:ResponseHeader>
		</ns4:ServiceResponse>
		<ns4:ServiceResponse>
			<ns4:ResponseHeader>
				<ns4:Service>BackOffice</ns4:Service>
				<ns4:Operation>FXDeal</ns4:Operation>
				<ns4:Status>${FXStatus}</ns4:Status>
				<ns4:Details>
					<ns4:Error>${FXError}</ns4:Error>
					<ns4:Warning>${FXWarning}</ns4:Warning>
					<ns4:Info>${FXInfo}</ns4:Info>
				</ns4:Details>
