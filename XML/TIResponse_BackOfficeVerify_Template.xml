<?xml version="1.0" standalone="yes"?>
<ns4:ServiceResponse xmlns="urn:messages.service.ti.apps.tiplus2.misys.com"
	xmlns:ns2="urn:common.service.ti.apps.tiplus2.misys.com" xmlns:ns3="urn:custom.service.ti.apps.tiplus2.misys.com"
	xmlns:ns4="urn:control.services.tiplus2.misys.com">
	<ns4:ResponseHeader>
		<ns4:Service>BackOffice</ns4:Service>
		<ns4:Operation>Verify</ns4:Operation>
		<ns4:Status>${Status}</ns4:Status>
		<ns4:Details>
			<ns4:Error>${HeaderError}</ns4:Error>
			<ns4:Warning>${HeaderWarning}</ns4:Warning>
			<ns4:Info>${HeaderInfo}</ns4:Info>
		</ns4:Details>
		<ns4:CorrelationId>${CorrelationId}</ns4:CorrelationId>
	</ns4:ResponseHeader>
	<BatchResponse>
		<ns4:ServiceResponse>
			<ns4:ResponseHeader>
				<ns4:Service>BackOffice</ns4:Service>
				<ns4:Operation>Exposure</ns4:Operation>
				<ns4:Status>${ExposureStatus}</ns4:Status>
				<ns4:Details>
