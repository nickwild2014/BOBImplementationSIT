<?xml version="1.0" standalone="yes"?>
<ServiceRequest xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
	xmlns:m='urn:messages.service.ti.apps.tiplus2.misys.com' xmlns:c='urn:common.service.ti.apps.tiplus2.misys.com'
	xmlns='urn:control.services.tiplus2.misys.com' xmlns:ns4='urn:custom.service.ti.apps.tiplus2.misys.com'>
	<RequestHeader>
		<Service>TI</Service>
		<Operation>Customer</Operation>
		<Credentials>
			<Name>${Name}</Name>
			<Password>${Password}</Password>
			<Certificate>${Certificate}</Certificate>
			<Digest>${Digest}</Digest>
		</Credentials>
		<ReplyFormat>FULL</ReplyFormat>
		<NoRepair>Y</NoRepair>
		<NoOverride>Y</NoOverride>
		<CorrelationId>${CorrelationId}</CorrelationId>
		<TransactionControl>NONE</TransactionControl>
	</RequestHeader>
	<m:Customer>
		<m:MaintType>${MaintType}</m:MaintType>
		<m:MaintainedInBackOffice>${MaintainedInBackOffice}</m:MaintainedInBackOffice>
		<m:SourceBankingBusiness>${SourceBankingBusiness}</m:SourceBankingBusiness>
		<m:Mnemonic>${Mnemonic}</m:Mnemonic>
		<m:CustomerNumber>${CustomerNumber}</m:CustomerNumber>
		<m:CustomerType>${CustomerType}</m:CustomerType>
		<m:FullName>${FullName}</m:FullName>
		<m:ShortName>${ShortName}</m:ShortName>
		<m:Reference>${Reference}</m:Reference>
		<m:MailToBranch>${MailToBranch}</m:MailToBranch>
		<m:Group>${Group}</m:Group>
		<m:GroupDescription>${GroupDescription}</m:GroupDescription>
		<m:AccountOfficer>${AccountOfficer}</m:AccountOfficer>
		<m:ResidenceCountry>${ResidenceCountry}</m:ResidenceCountry>
		<m:ParentCountry>${ParentCountry}</m:ParentCountry>
		<m:RiskCountry>${RiskCountry}</m:RiskCountry>
		<m:AnalysisCode>${AnalysisCode}</m:AnalysisCode>
		<m:Language>${Language}</m:Language>
		<m:Closed>${Closed}</m:Closed>
		<m:Blocked>${Blocked}</m:Blocked>
		<m:Deceased>${Deceased}</m:Deceased>
		<m:Inactive>${Inactive}</m:Inactive>
		<m:MidasFacilityAllow>${MidasFacilityAllow}</m:MidasFacilityAllow>
		<m:BankCode1>${BehalfOfBranch}</m:BankCode1>
		<m:BankCode2>${BankCode2}</m:BankCode2>
		<m:BankCode3>${BankCode3}</m:BankCode3>
		<m:BankCode4>${BankCode4}</m:BankCode4>
		<m:ClearingId>${ClearingId}</m:ClearingId>		
		<m:AddressDetails>
			<m:AddressDetail>
				<m:AddressType>${AddressDetailsAddressType}</m:AddressType>
				<m:AddressId>${AddressDetailsAddressId}</m:AddressId>
				<m:Salutation>${AddressDetailsSalutation}</m:Salutation>
				<m:NameAndAddress>${AddressDetailsNameAndAddress}</m:NameAndAddress>
				<m:ZipCode>${AddressDetailsZipCode}</m:ZipCode>
				<m:Language>${AddressDetailsLanguage}</m:Language>
				<m:Phone>${AddressDetailsPhone}</m:Phone>
				<m:Fax>${AddressDetailsFax}</m:Fax>
				<m:Telex>${AddressDetailsTelex}</m:Telex>
				<m:TelexAnswerBack>${AddressDetailsTelexAnswerBack}</m:TelexAnswerBack>
				<m:Email>${AddressDetailsEmail}</m:Email>
				<m:SwiftBIC>${AddressDetailsSwiftBIC}</m:SwiftBIC>
				<m:TransferMethod>${AddressDetailsTransferMethod}</m:TransferMethod>
				<m:AddresseeCustomerSBB>${AddressDetailsAddresseeCustomerSBB}</m:AddresseeCustomerSBB>
				<m:AddresseeCustomer>${AddressDetailsAddresseeCustomer}</m:AddresseeCustomer>
				<m:NumberOfCopies>${AddressDetailsNumberOfCopies}</m:NumberOfCopies>
				<m:NumberOfOriginals>${AddressDetailsNumberOfOriginals}
				</m:NumberOfOriginals>
			</m:AddressDetail>
		</m:AddressDetails>
		<m:SpecialInstructionDetails>
			<m:SpecialInstructionDetail>
				<m:Severity>${SpecialInstructionDetailsSeverity}</m:Severity>
				<m:Code>${SpecialInstructionDetailsCode}</m:Code>
				<m:Details>${SpecialInstructionDetailsDetails}</m:Details>
				<m:Style>${SpecialInstructionDetailsStyle}</m:Style>
				<m:Emphasis>${SpecialInstructionDetailsEmphasis}</m:Emphasis>
			</m:SpecialInstructionDetail>
		</m:SpecialInstructionDetails>
		<m:OtherDetails>
			<c:AllowMT103C>${OtherDetailsAllowMT103C}</c:AllowMT103C>
			<c:CutoffAmount>
				<c:Amount>${OtherDetailsCutoffAmountAmount}</c:Amount>
				<c:Currency>${OtherDetailsCutoffAmountCurrency}</c:Currency>
			</c:CutoffAmount>
			<c:SWIFTAckRequired>${OtherDetailsSWIFTAckRequired}</c:SWIFTAckRequired>
			<c:TransliterateSWIFT>${OtherDetailsTransliterateSWIFT}</c:TransliterateSWIFT>
			<c:Team>${OtherDetailsTeam}</c:Team>
			<c:CorporateAccess>${OtherDetailsCorporateAccess}</c:CorporateAccess>
			<c:PrincipalFxRateCode>${OtherDetailsPrincipalFxRateCode}</c:PrincipalFxRateCode>
			<c:ChargeFxRateCode>${OtherDetailsChargeFxRateCode}</c:ChargeFxRateCode>
			<c:AllowTaxExemptions>${OtherDetailsAllowTaxExemptions}</c:AllowTaxExemptions>
			<c:Suspended>${OtherDetailsSuspended}</c:Suspended>
		</m:OtherDetails>
		<m:SwiftDetails>
			<c:MainBankingEntity>${SwiftDetailsMainBankingEntity}</c:MainBankingEntity>
			<c:SwiftAddress>${SwiftDetailsSwiftAddress}</c:SwiftAddress>
			<c:Authenticated>${SwiftDetailsAuthenticated}</c:Authenticated>
			<c:Blocked>${SwiftDetailsBlocked}</c:Blocked>
			<c:Closed>${SwiftDetailsClosed}</c:Closed>
			<c:TransliterationRequired>${SwiftDetailsTransliterationRequired}</c:TransliterationRequired>
		</m:SwiftDetails>
		<m:CustomerExtraData>
			<ns4:LOB>${Lob}</ns4:LOB>
			<ns4:EMAILAD1>${Email_address_1}</ns4:EMAILAD1>
			<ns4:EMAILAD2>${Email_address_2}</ns4:EMAILAD2>
			<ns4:EMAILAD3>${Email_address_3}</ns4:EMAILAD3>
			<ns4:CRM1>${CRM1}</ns4:CRM1>
			<ns4:CRM2>${CRM2}</ns4:CRM2>
			<ns4:CRM3>${CRM3}</ns4:CRM3>
			<ns4:PANNO_Name>${Pan_number}</ns4:PANNO_Name>
			<ns4:CIFM_Name>${Cif_number}</ns4:CIFM_Name>
			<ns4:CUSTTYP_Name>${Client_type}</ns4:CUSTTYP_Name>
			<ns4:SOLID_Name>${Client_solid}</ns4:SOLID_Name>
			
			<ns4:COMPNAM>${Company_reg_no}</ns4:COMPNAM>
			<ns4:CITY>${City}</ns4:CITY>
			<ns4:STATE>${State}</ns4:STATE>
			<ns4:COUNTRY>${Country}</ns4:COUNTRY>
			<ns4:MOBILENOS>${Mobile_nos}</ns4:MOBILENOS>
			<ns4:TELAREA>${Telephone_area_code}</ns4:TELAREA>			
			<ns4:DOB>${DOB}</ns4:DOB>
			<ns4:DATOINC>${Date_of_incorporation}</ns4:DATOINC>
			<ns4:GENDER>${Gender}</ns4:GENDER>
			<ns4:INDNAPRE>${Individual_name_prefix}</ns4:INDNAPRE>
			<ns4:DRIVLICN>${Driver_lic_no}</ns4:DRIVLICN>
			<ns4:PASSPORT>${Passport_no}</ns4:PASSPORT>
			<ns4:UINNO>${UINNO}</ns4:UINNO>
			<ns4:VOTERID>${Voter_id}</ns4:VOTERID>
			<ns4:CREDTRAT>${Credit_rating}</ns4:CREDTRAT>
			<ns4:CREDITAS>${Credit_rating_as_on}</ns4:CREDITAS>
			<ns4:BORROWLE>${Borrowers_legal_constitutn}</ns4:BORROWLE>
			<ns4:INDUSTYP>${Industry_type}</ns4:INDUSTYP>
						
		</m:CustomerExtraData>
		<m:TICustomerExtraData>${TICustomerExtraData}</m:TICustomerExtraData>
	</m:Customer>
</ServiceRequest>
