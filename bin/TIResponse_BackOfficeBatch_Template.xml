<?xml version="1.0" standalone="yes"?>
<ServiceResponse xmlns:m='urn:messages.service.ti.apps.tiplus2.misys.com'
	xmlns='urn:control.services.tiplus2.misys.com' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'>
	<ResponseHeader>
		<Service>BackOffice</Service>
		<Operation>Batch</Operation>
		<Status>${Status}</Status>
		<Details>
			<Error>${Error}</Error>
			<Warning>${Warning}</Warning>
			<Info>${Info}</Info>
		</Details>
		<CorrelationId>${CorrelationId}</CorrelationId>
	</ResponseHeader>
	<BatchResponse>
		<ServiceResponse>
			<ResponseHeader>
				<Service>BackOffice</Service>
				<Operation>Exposure</Operation>
				<Status>${ExposureStatus}</Status>
				<Details>
					<Error>${ExposureError}</Error>
					<Warning>${ExposureWarning}</Warning>
					<Info>${ExposureInfo}</Info>
				</Details>
			</ResponseHeader>
		</ServiceResponse>
		<ServiceResponse>
			<ResponseHeader>
				<Service>BackOffice</Service>
				<Operation>Posting</Operation>
				<Status>${PostingStatus}</Status>
				<Details>
					<Error>${PostingError}</Error>
					<Warning>${PostingWarning}</Warning>
					<Info>${PostingInfo}</Info>
				</Details>
			</ResponseHeader>
		</ServiceResponse>
		<ServiceResponse>
			<ResponseHeader>
				<Service>BackOffice</Service>
				<Operation>FXContractDrawdown</Operation>
				<Status>${FXStatus}</Status>
				<Details>
					<Error>${FXError}</Error>
					<Warning>${FXWarning}</Warning>
					<Info>${FXInfo}</Info>
				</Details>
			</ResponseHeader>
		</ServiceResponse>
	</BatchResponse>
</ServiceResponse>